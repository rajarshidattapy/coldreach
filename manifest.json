{
  "manifest_version": 3,
  "name": "LinkedIn Cold DM Automator",
  "version": "1.0.0",
  "description": "Generate LinkedIn referral DMs for connected and non-connected profiles.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": ["clipboardWrite"],
  "content_scripts": [
  {
    "matches": ["https://www.linkedin.com/in/*"],
    "js": ["scrape.js"]
  }
]
}


